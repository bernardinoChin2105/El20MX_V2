
@{
    ViewBag.Title = "_CreateAccountModal";
}
<div class="modal-header">
    <div class="modal-title">
        <h3>Configuración de cuenta</h3>
    </div>

    <button type="button" class="close" data-dismiss="modal" aria-label="Cerrar">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    <div class="row">
        <div id="wizard" class="wizard-big">
            <h1>RFC</h1>

                <div class="col-md-12">
                    <div class="form-group">
                        <label>RCF</label>
                        
                        <input id="userName" name="userName" type="text" class="form-control" aria-required="true">
                    </div>
                    <div class="form-group">
                        <label>Contraseña</label>
                        
                        <input id="password" name="password" type="text" class="form-control" aria-required="true">
                    </div>

            </div>

            <h1>Cuentas bancarias</h1>
            <div>

            </div>
        </div>
    </div>
</div>