
@{
    Layout = null;
    Dictionary<string, string> formValues = (Dictionary<string, string>)ViewData["ResultValues"];
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Checkout Test Result</title>
</head>
<body>
    <div>
        @if (formValues != null)
        {
            foreach (KeyValuePair<string, string> dictValue in formValues)
            {
                <b>@dictValue.Key :</b> @dictValue.Value
                <br/>
            }
        }
    </div>
</body>
</html>
