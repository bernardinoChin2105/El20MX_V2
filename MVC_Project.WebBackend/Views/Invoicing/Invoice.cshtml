@model  MVC_Project.WebBackend.Models.InvoiceViewModel
@using MVC_Project.WebBackend.AuthManagement
@using MVC_Project.WebBackend.AuthManagement.Models
@{
    ViewBag.Title = "Factura";
    //Layout = "~/Views/Shared/_Layout.cshtml";
    var controllerName = HttpContext.Current.Request.RequestContext.RouteData.Values["controller"].ToString();
    AuthUser user = (AuthUser)Authenticator.AuthenticatedUser;
    var hasFullAccessController = user.HasFullAccessController(controllerName);

    //var exchange = Model.Currency == "MXN" ? "hide" : "";
}

@section styles{
    <link href="~/Content/template/plugins/iCheck/skins/square/blue.css" rel="stylesheet" />
    <style>
        table.invoices-table tr td {
            padding: 0px;
        }

            table.invoices-table tr td input.form-control {
                font-size: 11px;
                padding: 5px;
            }
    </style>
}

<div class="ibox-content p-xl">
    @using (Html.BeginForm("Invoice", "Invoicing", FormMethod.Post, new { @id = "InvoicingForm", @role = "form" }))
    {
        <div class="row">
            <div class="col-12">
                <h3>Información Emisor de Factura</h3>
                <hr />
            </div>
            <div class="col-md-2">
                <img src="@Model.Logo" class="img" id="logo" />
                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true)
            </div>
            <div class="col-md-5">
                <strong>@Model.BusinessName</strong><br>
                <strong>RFC: @Model.IssuingRFC</strong><br>
                <input type="hidden" name="IssuingRFC" id="IssuingRFC" value="@Model.IssuingRFC" />
                <input type="hidden" name="BusinessName" id="BusinessName" value="@Model.BusinessName" />
                <input type="hidden" name="IssuingTaxEmail" id="IssuingTaxEmail" value="@Model.IssuingTaxEmail" />
                <strong><abbr title="Email">E-mail:</abbr> @Model.IssuingTaxEmail</strong><br /><br />
                @*<strong>Régimen Fiscal: @Model.IssuingTaxRegime</strong><br>*@
                <div class="form-group row">
                    @Html.LabelFor(m => m.IssuingTaxRegime, new { @class = "col-form-label col-md-4 control-label" })
                    <div class="col-md-8">
                        @Html.DropDownListFor(model => model.IssuingTaxRegimeId, Model.ListTaxRegime, "Seleccione...", new { @class = "form-control chosen-select required" })
                        @*@Html.ValidationMessageFor(model => model.allyId)*@
                    </div>
                </div>
                <div class="form-group row hide is-hidden" id="PropertyAccNum">
                    @Html.LabelFor(m => m.PropertyAccountNumber, new { @class = "col-form-label col-md-4 control-label" })
                    <div class="col-md-8">
                        @Html.TextBoxFor(m => m.PropertyAccountNumber, new { @class = "form-control", @maxlength = "200", @autocomplete = "off" })
                    </div>
                </div>
                <div class="form-group row">
                    @Html.LabelFor(m => m.BranchOffice, new { @class = "col-form-label col-md-4 control-label" })
                    <div class="col-md-8">
                        @Html.DropDownListFor(model => model.BranchOffice, Model.ListBranchOffice, "Seleccione...", new { @class = "form-control chosen-select required" })
                        @*@Html.ValidationMessageFor(model => model.allyId)*@
                    </div>
                </div>
            </div>
            <div class="col-md-5">
                <div class="form-group row">
                    @Html.LabelFor(m => m.TypeInvoice, new { @class = "col-form-label col-md-4 control-label" })
                    <div class="col-md-8">
                        @Html.DropDownListFor(model => model.TypeInvoice, Model.ListTypeInvoices, "Seleccione...", new { @class = "form-control chosen-select required" })
                        @*@Html.ValidationMessageFor(model => model.allyId)*@
                    </div>
                </div>
                <div class="form-group row">
                    @Html.LabelFor(m => m.TypeRelationship, new { @class = "col-form-label col-md-4 control-label" })
                    <div class="col-md-8">
                        @Html.DropDownListFor(model => model.TypeRelationship, Model.ListTypeRelationship, "Seleccione...", new { @class = "form-control chosen-select required" })
                        @*@Html.ValidationMessageFor(model => model.allyId)*@
                    </div>
                    <div class="col-12">
                        <br />
                        <div class="i-checks">
                            <label class="control-label">
                                @Html.CheckBoxFor(m => m.InvoiceComplementChk, new { @class = "form-control", name = "current" })
                                @Html.DisplayNameFor(x => x.InvoiceComplementChk)
                            </label>
                        </div>
                    </div>
                    @Html.LabelFor(m => m.InvoiceComplement, new { @class = "col-form-label col-md-4 control-label complement hide" })
                    <div class="col-md-8">
                        @Html.TextBoxFor(m => m.InvoiceComplement, new { @class = "form-control complement hide", @maxlength = "200", @autocomplete = "off" })
                    </div>
                    <div class="col-12 complement hide">
                        <div id="CDFIS">
                            @*InvoicesCFDI lista*@
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-12">
                <br />
                <h3>Información Receptor de Factura</h3>
                <hr />
                <h5>Facturar a:</h5>
            </div>
            <div class="col-12">
                <div class="form-group row">
                    @Html.LabelFor(m => m.CustomerName, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.CustomerName, new { @class = "form-control", @maxlength = "200", @autocomplete = "off" })
                        <input type="hidden" name="CustomerId" id="CustomerId" />
                        <input type="hidden" name="TypeReceptor" id="TypeReceptor" />
                    </div>
                    @Html.LabelFor(m => m.RFC, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.RFC, new { @class = "form-control required", @maxlength = "200", @autocomplete = "off" })
                    </div>
                </div>
                <div class="form-group row">
                    @Html.LabelFor(m => m.Street, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.Street, new { @class = "form-control ", @maxlength = "200", @autocomplete = "off" })
                    </div>
                    @Html.LabelFor(m => m.OutdoorNumber, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.OutdoorNumber, new { @class = "form-control required", @maxlength = "200", @autocomplete = "off" })
                    </div>
                </div>
                <div class="form-group row">
                    @Html.LabelFor(m => m.InteriorNumber, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.InteriorNumber, new { @class = "form-control ", @maxlength = "200", @autocomplete = "off" })
                    </div>
                    @Html.LabelFor(m => m.Colony, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.DropDownListFor(model => model.Colony, Model.ListColony, "Seleccione...", new { @class = "form-control chosen-select required" })
                    </div>
                </div>
                <div class="form-group row">
                    @Html.LabelFor(m => m.ZipCode, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.ZipCode, new { @class = "form-control required", @maxlength = "200", @autocomplete = "off" })
                    </div>
                    @Html.LabelFor(m => m.Municipality, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.DropDownListFor(model => model.Municipality, Model.ListMunicipality, "Seleccione...", new { @class = "form-control chosen-select required" })
                    </div>
                </div>
                <div class="form-group row">
                    @Html.LabelFor(m => m.State, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.DropDownListFor(model => model.State, Model.ListState, "Seleccione...", new { @class = "form-control chosen-select required" })
                    </div>
                    @Html.LabelFor(m => m.Country, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.DropDownListFor(model => model.Country, Model.ListCountry, "Seleccione...", new { @class = "form-control chosen-select required" })
                    </div>
                </div>
                <div class="form-group row">
                    @Html.LabelFor(m => m.CustomerEmail, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.CustomerEmail, new { @class = "form-control required", @maxlength = "200", @autocomplete = "off" })
                        @*@Html.DropDownListFor(model => model.CustomerEmailId, Model.ListCustomerEmail, "Seleccione...", new { @class = "form-control chosen-select required" })*@
                    </div>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-12">
                <br />
                <h3>Datos Fiscales para Facturar:</h3>
                <hr />
            </div>

            <div class="col-12">
                <div class="form-group row">
                    @*@Html.LabelFor(m => m.TypeVoucherId, new { @class = "col-form-label col-md-2 control-label" })
                        <div class="col-md-4">
                            @Html.DropDownListFor(model => model.TypeVoucherId, Model.ListTypeVoucher, "Seleccione...", new { @class = "form-control chosen-select required" })
                        </div>*@
                    @Html.LabelFor(m => m.SerieFolio, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.SerieFolio, new { @class = "form-control ", @maxlength = "200", @autocomplete = "off", disabled = "disabled" })
                        @Html.HiddenFor(m => m.Serie)
                        @Html.HiddenFor(m => m.Folio)
                    </div>
                    @Html.LabelFor(m => m.UseCFDI, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.DropDownListFor(model => model.UseCFDI, Model.ListUseCFDI, "Seleccione...", new { @class = "form-control chosen-select required" })
                    </div>
                </div>
                <div class="form-group row">
                    @Html.LabelFor(m => m.PaymentForm, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.DropDownListFor(model => model.PaymentForm, Model.ListPaymentForm, "Seleccione...", new { @class = "form-control chosen-select required" })
                    </div>
                    @Html.LabelFor(m => m.PaymentMethod, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.DropDownListFor(model => model.PaymentMethod, Model.ListPaymentMethod, "Seleccione...", new { @class = "form-control chosen-select required" })
                    </div>
                </div>
                <div class="form-group row">
                    @Html.LabelFor(m => m.Currency, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.DropDownListFor(model => model.Currency, Model.ListCurrency, "Seleccione...", new { @class = "form-control chosen-select required" })
                    </div>

                    @if (Model.Currency == "MXN")
                    {
                        @Html.LabelFor(m => m.ExchangeRate, new { @class = "col-form-label col-md-2 control-label exchange hide" })
                        <div class="col-md-4">
                            @Html.TextBoxFor(m => m.ExchangeRate, new { @class = "form-control exchange hide", @maxlength = "200", @autocomplete = "off" })
                        </div>
                    }
                    else
                    {
                        @Html.LabelFor(m => m.ExchangeRate, new { @class = "col-form-label col-md-2 control-label exchange required" })
                        <div class="col-md-4">
                            @Html.TextBoxFor(m => m.ExchangeRate, new { @class = "form-control exchange required", @maxlength = "200", @autocomplete = "off" })
                        </div>
                    }
                </div>
                <div class="form-group row">
                    <div class="col-12 col-md-6">
                        <div class="i-checks">
                            <label class="control-label">
                                @Html.CheckBoxFor(m => m.InternationalChk, new { @class = "form-control" })
                                @Html.DisplayNameFor(x => x.InternationalChk)
                            </label>
                        </div>
                    </div>
                    <div class="col-12 col-md-6">
                        <div class="i-checks">
                            <label class="control-label">
                                @Html.CheckBoxFor(m => m.TaxesChk, new { @class = "form-control", name = "current" })
                                @Html.DisplayNameFor(x => x.TaxesChk)
                            </label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-12 international hide">
                <hr />
                <h4>Comercio Internacional: </h4>
            </div>
            <div class="col-12 international hide">
                @*<div class="form-group row">
                        @Html.LabelFor(m => m.CustomsPatent, new { @class = "col-form-label col-md-2 control-label" })
                        <div class="col-md-4">
                            @Html.DropDownListFor(model => model.CustomsPatent, Model.ListCustomsPatent, "Seleccione...", new { @class = "form-control chosen-select" })
                        </div>
                        @Html.LabelFor(m => m.Customs, new { @class = "col-form-label col-md-2 control-label" })
                        <div class="col-md-4">
                            @Html.DropDownListFor(model => model.Customs, Model.ListCustoms, "Seleccione...", new { @class = "form-control chosen-select" })
                        </div>
                    </div>*@
                <div class="form-group row">
                    @Html.LabelFor(m => m.MotionNumber, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.MotionNumber, new { @class = "form-control", @maxlength = "200", @autocomplete = "off" })
                        @*@Html.DropDownListFor(model => model.MotionNumber, Model.ListMotionNumber, "Seleccione...", new { @class = "form-control chosen-select" })*@
                    </div>
                </div>
            </div>
        </div>

        <div id="taxes" class="row hide is-hidden">
            <div class="col-12">
                <br />
                <h3>Impuestos</h3>
                <hr />
            </div>
            <div class="col-12">
                <div class="form-group row">
                    @Html.LabelFor(m => m.Withholdings, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.DropDownListFor(model => model.Withholdings, Model.ListWithholdings, "Seleccione...", new { @class = "form-control chosen-select" })
                    </div>
                    @Html.LabelFor(m => m.Transferred, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.DropDownListFor(model => model.Transferred, Model.ListTransferred, "Seleccione...", new { @class = "form-control chosen-select" })
                    </div>
                </div>
                <div class="form-group row">
                    @Html.LabelFor(m => m.Valuation, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.DropDownListFor(model => model.Valuation, Model.ListValuation, "Seleccione...", new { @class = "form-control chosen-select" })
                    </div>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-12">
                <br />
                <h3>Condiciones y Comentarios a Facturar</h3>
                <hr />
            </div>
            <div class="col-12">
                <div class="form-group row">
                    @Html.LabelFor(m => m.PaymentConditions, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.PaymentConditions, new { @class = "form-control ", @maxlength = "200", @autocomplete = "off" })
                    </div>
                    @Html.LabelFor(m => m.DiscountRate, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.DiscountRate, new { @class = "form-control money", @maxlength = "200", @autocomplete = "off" })
                    </div>
                </div>
                <div class="form-group row">
                    @Html.LabelFor(m => m.Comments, new { @class = "col-form-label col-md-2 control-label" })
                    <div class="col-md-10">
                        @Html.TextAreaFor(m => m.Comments, new { @class = "form-control ", @maxlength = "200", @autocomplete = "off" })
                        @*@Html.TextBoxFor(m => m.DiscountRate, new { @class = "form-control ", @maxlength = "200", @autocomplete = "off" })*@
                    </div>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-12">
                <br />
                <h3>Productos y/o Servicios a facturar:</h3>
                <hr />
            </div>
            <div class="col-md-4 offset-8 text-right">
                <!-- Button trigger modal -->
                <button type="button" class="btn btn-color" data-toggle="modal" data-target="#ServProdModal">Agregar nuevo Servicio o Producto</button>
            </div>
        </div>

        <div class="table-responsive m-t">
            <table id="table" class="table table-responsive invoices-table">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Cantidad</th>
                        <th>Código SAT</th>
                        <th>Descripción Producto o Servicio</th>
                        <th>Unidad SAT</th>
                        <th>Unidad</th>
                        <th>Precio Unitario</th>
                        <th>% Descuento</th>
                        <th>
                            Impuesto
                            <small>(IVA)</small>
                        </th>
                        <th>
                            Impuesto
                            <small>(IEPS)</small>
                        </th>
                        <th>SubTotal</th>
                        <th>Opciones</th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model.ProductServices.Count > 0)
                    {
                        var index = 1;
                        foreach (var item in Model.ProductServices)
                        {
                            <tr>
                                <td>@index</td>
                                <td>@item.Quantity</td>
                                <td>@item.SATCode</td>
                                <td>@item.ProductServiceDescription</td>
                                <td>@item.SATUnit</td>
                                <th>@item.Unit</th>
                                <td>@item.UnitPrice</td>
                                <td>@item.DiscountRateProServ</td>
                                <td>@item.TaxesIVA</td>
                                <td>@item.TaxesIEPS</td>
                                <td>@item.Subtotal</td>
                                <td class=""></td>
                            </tr>
                            @(index++);
                        }
                    }
                </tbody>
                <tfoot>
                    <tr>
                        <th colspan="9" style="text-align:right">
                            @Html.LabelFor(m => m.Subtotal, new { @class = " control-label" }):
                            @Html.HiddenFor(m => m.Subtotal)
                        </th>
                        <th colspan="2" id="lblSubtotal">
                            @Model.Subtotal.ToString("C2")
                        </th>
                    </tr>
                    <tr class="trDiscount">
                        <th colspan="9" style="text-align:right">
                            @Html.LabelFor(m => m.TotalDiscount, new { @class = " control-label" }):
                            @Html.HiddenFor(m => m.TotalDiscount)
                        </th>
                        <th colspan="9" id="lblTotalDiscount">@Model.TotalDiscount.ToString("C2")</th>
                    </tr>
                    <tr class="trTaxT">
                        <th colspan="9" style="text-align:right">
                            @Html.LabelFor(m => m.TaxTransferred, new { @class = " control-label" }):
                            @Html.HiddenFor(m => m.TaxTransferred)
                        </th>
                        <th colspan="2" id="lblTaxTransferred">@Model.TaxTransferred.ToString("C2")</th>
                    </tr>
                    <tr class="trTaxWIVA">
                        <th colspan="9" style="text-align:right">
                            @Html.LabelFor(m => m.TaxWithheldIVA, new { @class = " control-label" }):
                            @Html.HiddenFor(m => m.TaxWithheldIVA)
                        </th>
                        <th colspan="2" id="lblTaxWithheldIVA">@Model.TaxWithheldIVA.ToString("C2")</th>
                    </tr>
                    <tr class="trTaxWISR">
                        <th colspan="9" style="text-align:right">
                            @Html.LabelFor(m => m.TaxWithheldISR, new { @class = " control-label" }):
                            @Html.HiddenFor(m => m.TaxWithheldISR)
                        </th>
                        <th colspan="2" id="lblTaxWithheldISR">@Model.TaxWithheldISR.ToString("C2")</th>
                    </tr>
                    <tr>
                        <th colspan="9" style="text-align:right">
                            @Html.LabelFor(m => m.Total, new { @class = " control-label" }):
                            @Html.HiddenFor(m => m.Total)
                        </th>
                        <th colspan="2" id="lblTotal">@Model.Total.ToString("C2")</th>
                    </tr>
                </tfoot>
            </table>
        </div><!-- /table-responsive -->

        <div class="row">
            <div class="col-12">
                <div class="form-group row pull-right">
                    <div class="col-md-10 offset-2 text-right">
                        <a href="#" class='btn btn-white'><i class="fa fa-times"></i> Cancelar</a>
                        @*<a href='@Url.Action("Index", "Plan")' class='btn btn-white'><i class="fa fa-times"></i> Cancelar</a>*@
                        <button type="button" id="SavedInvoice" class="btn btn-color"> <i class="fa fa-save"></i> Guardar </button>
                        <button type="button" id="validarDatos" class="btn btn-color"> <i class="fa fa-save"></i> Timbrar Factura </button>
                    </div>
                </div>
            </div>
        </div>
    }
</div>

<!-- Modal -->
<div class="modal fade" id="ServProdModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            @using (Html.BeginForm("Invoice", "Invoicing", FormMethod.Post, new { @id = "ConceptForm", @role = "form" }))
            {
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Agregar Servicio o Producto</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <input type="hidden" class="form-control" name="Row" id="Row">
                    <div class="form-group row">
                        <label class="control-label col-md-3">Cantidad</label>
                        <div class="col-md-9">
                            <input type="number" class="form-control required" name="Quantity" id="Quantity" autocomplete="off">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="control-label col-md-3">Concepto</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control required" name="SATCode" id="SATCode" autocomplete="off" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="control-label col-md-3">Descripción</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control required" name="ProductServiceDescription" id="ProductServiceDescription" autocomplete="off" maxlength="150" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="control-label col-md-3">Unidad</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control required" name="SATUnit" id="SATUnit" autocomplete="off" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="control-label col-md-3">Precio Unitario</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control required money" name="UnitPrice" id="UnitPrice" autocomplete="off" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="control-label col-md-3">% Descuento</label>
                        <div class="col-md-9">
                            @*<input type="text" class="form-control money" name="DiscountRate" id="DiscountRate" autocomplete="off" />*@
                            <input type="text" class="form-control money" name="DiscountRateProServ" id="DiscountRateProServ" autocomplete="off" />
