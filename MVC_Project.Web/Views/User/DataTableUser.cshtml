@model MVC_Project.Web.Models.UserData
<script type="text/javascript">
    $(document).ready(function () {

        var columnsMetaData = [
            { data: "@Html.NameFor(x=>x.Id)", visible: false },
            { data: "@Html.NameFor(x => x.Email)", title: "@Html.DisplayNameFor(x => x.Email)" },
            { data: "@Html.NameFor(x => x.RoleName)", title: "@Html.DisplayNameFor(x => x.RoleName)" },
            { data: "@Html.NameFor(x => x.Name)", title: "@Html.DisplayNameFor(x => x.Name)" },
            { data: "@Html.NameFor(x => x.CreatedAt)", title: "@Html.DisplayNameFor(x => x.CreatedAt)" },
            { data: "@Html.NameFor(x => x.UpdatedAt)", title: "@Html.DisplayNameFor(x => x.UpdatedAt)" },
        ];
        var controlador = new UserIndexControlador({
            columns: columnsMetaData,
            dataTableId: "#table",
            formId: "#SearchForm"
        });
        controlador.init();
    });
</script>

